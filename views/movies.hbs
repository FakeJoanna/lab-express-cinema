<link rel="stylesheet" href="/stylesheets/movies.css">

<h1>Ironhack Cinema</h1>
<p>Click on the movies to check the showtimes!</p>
<div id="movieDiv">
    {{#each movies}}
        <div class="movie">
            <img src="{{image}}" alt="">
            <h3>{{title}}</h3>
            <a href="/movies/{{_id}}">See more</a>
        </div>
    {{/each}}
</div>
